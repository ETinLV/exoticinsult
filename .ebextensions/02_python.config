option_settings:
  "aws:elasticbeanstalk:application:environment":
    DJANGO_SETTINGS_MODULE: "exoticinsult.production"
    "PYTHONPATH": "/opt/python/current/app/exoticinsult:$PYTHONPATH"
  "aws:elasticbeanstalk:container:python":
    WSGIPath: exoticinsult/wsgi.py
    NumProcesses: 3
    NumThreads: 20
  "aws:elasticbeanstalk:container:python:staticfiles":
    "/static/": "www/static/"